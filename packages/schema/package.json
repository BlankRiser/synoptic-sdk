{
	"name": "@devhaven/schema",
	"license": "MIT",
	"version": "0.1.0",
	"module": "index.ts",
	"type": "module",
	"exports": {
		"./requests": "./src/requests/*.ts",
		"./responses": "./src/responses/*.ts"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"synoptic",
		"weather",
		"api",
		"sdk",
		"typescript"
	],
	"scripts": {
		"dev": "bun run ./src/index.ts",
		"build": "bun run ./scripts/build.ts",
		"check": "biome check ./src",
		"check:fix": "biome check ./src --write",
		"lint": "biome lint ./src"
	},
	"peerDependencies": {
		"typescript": "5.9.3"
	},
	"dependencies": {
		"zod": "4.1.12"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.2"
	}
}
